<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:wxq="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.cshen.tiyu.activity.LotteryTypeActivity" >
<com.cshen.tiyu.widget.TopViewLeft
        android:id="@+id/tv_head"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        wxq:topname="" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

            <WebView
                android:id="@+id/wv_latest_lottery"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" />

            <TextView 
                android:id="@+id/textcall"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:visibility="gone"
                android:text =" 若还有疑问，可咨询客服"
                android:textColor="#000000"
                android:textSize="16dp"
                android:paddingBottom="10dp"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>